{
  "manifest_version": 3,
  "name": "Website Link QR Code Generator",
  "version": "1.0",
  "description": "Convert current webpage link to QR code",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  }
}
